{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@utility/*": [
        "./src/utility/*"
      ]
    }
  },
}